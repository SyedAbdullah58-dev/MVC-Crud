@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}
<h1>Index Category</h1>

<div class="container p-3">
    <div class="row pt-4"> test</div>
</div>

<table class="table table-bordered table-stripped">
    <thead>
        <tr>

            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th>
 </tr>

        @foreach(var obj in Model)
        {
            <tr>

                <td width="50%">
                    @obj.Name
                </td>
                <td width = "50%">
                    @obj.DisplayOrder
                </td>
            </tr>

        }

    </thead>



</table>
